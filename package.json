    "dev": "node build/script.dev.js",
    "build": "node build/script.build.js",
    "lint": "eslint --ext .js,.vue src",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "anchorme": "^1.1.2",
    "date-fns": "2.0.0-alpha.7",
    "express": "^4.15.4",
