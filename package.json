    "html2canvas": "^0.5.0-beta4",
    "morgan": "^1.8.2",
    "vue-qr": "^1.2.3",
    "vuex": "^3.0.0",
    "vuex-persist": "^0.3.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.13",
    "@storybook/addon-links": "^3.2.13",
    "@storybook/vue": "^3.2.13",
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "colors": "^1.1.2",
    "connect-history-api-fallback": "^1.1.0",
    "css-loader": "^0.28.0",
    "es6-promise": "^4.1.1",
